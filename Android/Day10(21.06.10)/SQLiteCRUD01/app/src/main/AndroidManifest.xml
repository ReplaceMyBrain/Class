<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khankong.sqlitecrud01">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLiteCRUD01">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectAllActivity"
            android:label="SelectAllActivity">
        </activity>
        <activity android:name=".InsertActivity"
            android:label="InsertActivity">
        </activity>
        <activity android:name=".UpdateActivity"
            android:label="UpdateActivity">
        </activity>
        <activity android:name=".DeleteActivity"
            android:label="DeleteActivity">
        </activity>

    </application>

</manifest>